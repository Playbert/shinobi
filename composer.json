{
    "name": "playbert/shinobi",
    "description": "Laravel Roles and Permissions",
    "keywords": ["roles", "permissions", "auth", "laravel"],
    "license": "MIT",
    "authors": [{
        "name": "Playbert Ronaldo",
        "email": "playbert.ronaldo@gmail.com"
    }],
    "require": {
        "php" : "^7.3|^8.0|^8.1",
        "illuminate/support": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Playbert\\Shinobi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Playbert\\Shinobi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Playbert\\Shinobi\\ShinobiServiceProvider"
            ],
            "aliases": {
                "Shinobi": "Playbert\\Shinobi\\Facades\\Shinobi"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "7.*",
        "doctrine/dbal": "3.4.*"
    }
}
