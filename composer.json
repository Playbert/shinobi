{
    "name": "PRPP",
    "description": "Laravel Roles and Permissions",
    "keywords": ["roles", "permissions", "auth", "laravel"],
    "license": "MIT",
    "authors": [{
        "name": "Playbert Ronaldo",
        "email": "playbert.ronaldo@gmail.com"
    }],
    "require": {
         "php" : "^7.3|^8.0|^8.1",
        "illuminate/auth": "^7.0|^8.0|^9.0",
        "illuminate/container": "^7.0|^8.0|^9.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.4",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Caffeinated\\Shinobi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caffeinated\\Shinobi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinated\\Shinobi\\ShinobiServiceProvider"
            ],
            "aliases": {
                "Shinobi": "Caffeinated\\Shinobi\\Facades\\Shinobi"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "doctrine/dbal": "^2.10@dev"
    }
}
